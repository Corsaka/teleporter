{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",
    "provides": [
      "cosmic_quilt"
    ],

    "intermediate_mappings": "net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "A mod that adds teleport blocks.",
      "contributors": {
        "Corsaka": "Owner"
      },

      "contact": {
        "homepage": "https://github.com/Corsaka/teleporter",
        "issues": "https://github.com/Corsaka/teleporter/issues",
        "sources": "https://github.com/Corsaka/teleporter"
      },

      "icon": "assets/cosmicquilt/icon.png"
    },

    "depends": [
      {
        "id": "quilt_loader",
        "versions": "*"
      },
      {
        "id": "cosmic_reach",
        "versions": "*"
      }
    ]
  },

  "mixin": "teleporter.mixins.json"
}